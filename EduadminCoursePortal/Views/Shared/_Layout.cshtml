@using Microsoft.AspNetCore.Mvc.Localization
@using Microsoft.Extensions.Localization

@inject IStringLocalizer<EduadminCoursePortal.SharedResources> Localizer
    
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EduadminCoursePortal</title>

    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/lib/jquery-ui-1.12.1/jquery-ui.min.css" />
    <link rel="stylesheet" href="~/lib/jquery-ui-1.12.1/jquery-ui.structure.min.css" />
    <link rel="stylesheet" href="~/lib/jquery-ui-1.12.1/jquery-ui.theme.min.css" />
    <link rel="stylesheet" href="~/css/site.min.css" />
    <script src="~/npm/jquery.min.js"></script>
    <script src="~/npm/bootstrap.min.js"></script> <!--Eventuellt går den här att ta bort?-->
    <script src="~/js/site.min.js" asp-append-version="true"></script>
    <script src="~/npm/jquery.validate.min.js"></script>
    <script src="~/npm/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/npm/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/js/jquery.dynamic.content.validator.js"></script>
    <script asp-append-version="true" src="~/js/Validation.js"></script>
</head>
<body>
    <div class="container body-content">
        <br />
        @if (ViewBag.BackUrl != null)
        {
            <a href="@ViewBag.BackUrl">@("<-") @Localizer["Back"]</a>
        }
        <br />
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2017 - Multinet</p>
        </footer>
    </div>
    <script src="~/lib/jquery-ui-1.12.1/jquery-ui.min.js"></script>
    <script src="~/js/jquery.mask.js"></script>
    <script asp-append-version="true" src="~/js/allscripts.min.js"></script>
    <script src="~/js/site.min.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
